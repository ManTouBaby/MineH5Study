@CHARSET "UTF-8";

html, body, #mainWindow {
	font-family: sans-serif;
	height: 100%;
	width: 100%;
}

html, body {
	margin: 0;
	padding: 0;
}

#divTable {
	background-color: white;
	position: absolute;
	top: 100px;
	width: 320px;
	height: 280px;
	z-index: 99999;
	border-color: #ccc;
	border-style: solid;
	border-width: 0 1px 1px 0;
	border-radius: 3px;
	box-shadow: 5px 5px 10px #999;
	font-size: 14px;
	color: #666;
	display: none;
}

#mainWindow {
	visibility: hidden;
}

.dutyTitle {
	color: #316989;
}

.dutyContent {
	color: #666666
}

.title {
    background: url() no-repeat center bottom;
     line-height: 20px;
     height:20px;
     font-size: 12px;
     color: #FFF;
}
.esriPopup .titlePane {
       color: #FFFFFF;
    line-height: 20px;
        cursor: default;
}
.esriPopup {
    font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
    font-size: 12px;
}

.titlePane {
	background-color: #2674ac !important
}

.littleTxt {
	font-size: 12px;
}
