<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title></title>
		<link rel="stylesheet" type="text/css" href="../plugins/layer/theme/default/layer.css" />
		<!-- Bootstrap 3.3.7 -->
		<link rel="stylesheet" type="text/css" href="../plugins/bootstrap/css/bootstrap.min.css" />
		<!-- Font Awesome -->
		<link rel="stylesheet" type="text/css" href="../plugins/font_awesome/css/font-awesome.min.css" />
		<!-- Ionicons -->
		<link rel="stylesheet" type="text/css" href="../plugins/ionicons/css/ionicons.min.css" />
		<!-- AdminLTE -->
		<link rel="stylesheet" type="text/css" href="../plugins/AdminLTE/css/AdminLTE.css" />
		<!-- AdminLTE skin -->
		<link rel="stylesheet" type="text/css" href="../plugins/AdminLTE/css/skins/_all-skins.min.css" />
		<link rel="stylesheet" href="../plugins/ivew/dist/styles/iview.css" />
		<!-- general -->
		<link rel="stylesheet" type="text/css" href="../css/general.css" />
		<link rel="stylesheet" type="text/css" href="../css/xCommon.css" />
		
		<script type="text/javascript" src="../plugins/vue/dist/vue.js"></script>
		<script type="text/javascript" src="../plugins/ivew/dist/iview.js"></script>
		<style>
			
			html, body{
				position:relative;
				width: 100%;
				height:100%;
			}
			.popup-container {
				display: block;
				width: 100%;
				height: 100%;
				padding: 15px 10px;
			}
			.popup-content {
				position: relative;
				display: block;
				width: 100%;
				height: 100%;
				padding: 15px 10px;
			}
			
			
		</style>
		
		<style>
			.time {
				font-size: 14px;
				font-weight: bold;
			}
			.content {
				padding-left: 10px;
			}
			.ivu-input[disabled], fieldset[disabled] .ivu-input {
				background-color: #e4e4e4;
				color:#000;
			}
			
			.ivu-input-group-append .ivu-btn.ivu-btn-info {
				background: #57c5f7;
				/*font-size: 20px;*/
				color:#fff;
			}
			.ivu-form-item {
				margin-bottom: 14px;
			}
		</style>
		
	</head>
	<body class="innerPage">
		<div id="main">
			<section class="content">
				<div class="box box-primary">
					<div class="box-header with-border">
						<h3 class="box-title">执勤巡组：{{barrierName}} </h3>
					</div>
					<div class="box-body">
						<i-form :label-width="100">
							<Row :gutter="16">
								<template v-for="param in barrierParams">
									<i-col span="12">
										<form-item :label="param.label">
											<i-input v-model="param.value" disabled></i-input>
										</form-item>
									</i-col>
								</template>
								
							</Row>
							
						</i-form>
					</div>
					
				</div>
				
				<div class="box box-primary">
					<div class="box-header with-border">
						<h3 class="box-title">执勤警员：{{policeName}}</h3>
					</div>
					<div class="box-body">
						<i-form :label-width="100">
							<Row :gutter="16">
									<i-col span="12" v-for="param in policeParams">
										<form-item :label="param.label">
											<template v-if="param.label == '电话号码'">
												<i-input v-model="param.value" disabled>
													{{param.value}}
													<i-button slot="append" icon="ios-telephone" type="info"></i-button>
												</i-input>
											</template>
											<template v-else>
												<i-input v-model="param.value" disabled>{{param.value}}</i-input>
											</template>
										</form-item>
									</i-col>
								
							</Row>
							
						</i-form>
					</div>
					
				</div>
				
				
			</section>	
		</div>
		
		
		
		
		<script src="../plugins/jquery/jquery-3.2.1.js"></script>
		<script type="text/javascript" src="../js/common/common.js" ></script>
		<script type="text/javascript" src="../js/qwDubian-vue.js" ></script>
	</body>
</html>
