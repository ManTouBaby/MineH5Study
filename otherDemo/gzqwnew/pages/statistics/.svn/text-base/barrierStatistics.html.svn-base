		<script type="text/jscript" src="../../plugins/highcharts/highcharts.js"></script>

		<script>
			$(document).ready(function() {
				var colors = Highcharts.getOptions().colors,
					categories = ['越秀', ' 天河', '  荔湾', '海珠', '黄埔'],
					data = [{
						y: 25,
						color: colors[0],
						drilldown: {
							name: '越秀区',
							categories: ['路面', '卡点', '视频', '重点目标','社区'],
							data: [10, 10, 10, 10, 10],
							color: colors[0]
						}
					}, {
						y: 20,
						color: colors[1],
						drilldown: {
							name: '天河区',
							categories: ['路面', '卡点', '视频', '重点目标','社区'],
							data: [10, 10, 10, 10, 10],
							color: colors[1]
						}
					}, {
						y: 25,
						color: colors[2],
						drilldown: {
							name: '荔湾区',
							categories: ['路面', '卡点', '视频', '重点目标','社区'],
							data: [10, 10, 10, 10, 10],
							color: colors[2]
						}
					}, {
						y: 15,
						color: colors[3],
						drilldown: {
							name: '海珠区',
							categories: ['路面', '卡点', '视频', '重点目标','社区'],
							data: [10, 10, 10, 10, 10],
							color: colors[3]
						}
					}, {
						y: 15,
						color: colors[4],
						drilldown: {
							name: '黄埔区',
							categories: ['路面', '卡点', '视频', '重点目标','社区'],
							data: [10, 10, 10, 10, 10],
							color: colors[4]
						}
					}],
					browserData = [],
					versionsData = [],
					i,
					j,
					dataLen = data.length,
					cdrillDataLen,
					brightness;
				// 构建数据数组
				for(i = 0; i < dataLen; i += 1) {
					// 添加浏览器数据
					browserData.push({
						name: categories[i],
						y: data[i].y,
						color: data[i].color
					});
					// 添加版本数据
					drillDataLen = data[i].drilldown.data.length;
					for(j = 0; j < drillDataLen; j += 1) {
						brightness = 0.2 - (j / drillDataLen) / 5;
						versionsData.push({
							name: data[i].drilldown.categories[j],
							y: data[i].drilldown.data[j],
							color: Highcharts.Color(data[i].color).brighten(brightness).get()
						});
					}
				}
				// 创建图表
				$('#container').highcharts({
					chart: {
						type: 'pie'
					},
					title: {
						text: '全市岗位信息分布'
					},
					subtitle: {
						text: '各区岗位信息汇总'
					},
					yAxis: {
						title: {
							text: '总百分比'
						}
					},
					plotOptions: {
						pie: {
							shadow: false,
							center: ['50%', '50%']
						}
					},
					tooltip: {
						valueSuffix: '%'
					},
					series: [{
						name: '浏览器',
						data: browserData,
						size: '60%',
						dataLabels: {
							formatter: function() {
								return this.y > 5 ? this.point.name : null;
							},
							color: 'white',
							distance: -30
						}
					}, {
						name: '百分比',
						data: versionsData,
						size: '80%',
						innerSize: '60%',
						/* dataLabels: {
						     formatter: function () {
						         // 大于1则显示
						         return this.y > 1 ? '<b>' + this.point.name + ':</b> ' + this.y + '%'  : null;
						     }
						 }*/
					}]
				});
			});
